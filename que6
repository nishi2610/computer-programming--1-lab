#include<stdio.h>
void main()
{
printf("enter marks\n");
int x,y,z,m;
scanf("%d,%d,%d",&x,&y,&z);
m=x+y+z/3
if(m>=70)
{
    printf("distinction");
}
else if(m>=60)
{
    printf("first class");
}
else if(m>=50)
{
    printf("second class");
}
else if(m>=35)
{
    printf("third class");
}
else
{
    printf("fail");
}
}
